QWidget#box2>QLabel, #btn1,#btn2{
    background-color: pink;

}

QWidget:!focus{
    border: 2px dotted green;
}

QWidget:disabled {
    font-size: 26px;
    background-color: orange;

}

QWidget:enabled {
    font-size: 16px;
    background-color: lightgray;
}

QPushButton:hover {
    border: 2px solid red;
}

QPushButton:pressed {
    border: 2px dotted orange;
}

QCheckBox:checked, QCheckBox:hover {
    background-color: red;
}

QCheckBox::indicator {
    width: 40px;
    height: 30px;

}

QCheckBox::indicator:on {
    image: url(xxx.png);
}

QCheckBox::indicator:indeterminate {
    image: url(zzz.png);
}

QCheckBox::indicator:off {
    image: url(yyy.png);
}
